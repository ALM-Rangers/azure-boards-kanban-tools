{
    "linterOptions": {
        "exclude": [
            "bin",
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    },
    "rules": {
        "quotemark": {
            "options": [
              "double",
              "jsx-double"
            ]
          },
          "member-access": false,
          "object-literal-sort-keys": false,
          "array-type": false,
          "ordered-imports": false,
          "no-shadowed-variable": false,
          "no-console": false,
          "max-classes-per-file": false
    }
}