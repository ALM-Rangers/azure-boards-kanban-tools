{
  "name": "kanban-board-tools",
  "private": true,
  "version": "0.0.0",
  "main": "webpack.config.js",
  "scripts": {
    "clean:vsix": "rimraf *.vsix",
    "clean": "npm run clean:vsix && rimraf dist",
    "dev": "webpack-dev-server -d --hot --progress --colors --https --content-base ./src/ --port 9090",
    "lint": "tslint -c tslint.json \"src/*.ts\"",
    "package:dev": "npm run clean:vsix && node ./scripts/packageDev",
    "publish:dev": "npm run package:dev && node ./scripts/packageDev",
    "build:release": "npm run lint && npm run clean && mkdir dist && webpack --progress --colors --config webpack.prod.config.js --output-path ./dist -p",
    "package:release": "npm run clean:vsix && npm run build:release && node ./scripts/packageRelease",
    "test": "snyk test && karma start --single-run"
  },
  "devDependencies": {
    "@types/jquery": "^3.2.18",
    "@types/react": "^16.0.41",
    "@types/react-dom": "^16.0.11",
    "copy-webpack-plugin": "^4.6.0",
    "cpx": "^1.5.0",
    "css-loader": "^3.0.0",
    "file-loader": "^1.1.6",
    "node-sass": "^4.12.0",
    "react": "~16.2.0",
    "react-dom": "^16.2.1",
    "rimraf": "^2.6.3",
    "sass-loader": "^6.0.7",
    "snyk": "^1.192.4",
    "style-loader": "^0.19.1",
    "tfx-cli": "^0.7.8",
    "ts-loader": "^3.5.0",
    "tslint": "^5.18.0",
    "tslint-loader": "^3.5.4",
    "typescript": "~2.6.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.5"
  },
  "dependencies": {
    "@uifabric/icons": "^5.8.0",
    "applicationinsights-js": "^1.0.20",
    "es6-promise": "^4.2.8",
    "office-ui-fabric-react": "^5.34.3",
    "telemetryclient-team-services-extension": "^1.0.10",
    "vss-ui": "^3.127.3",
    "vss-web-extension-sdk": "~4.126.2"
  }
}
