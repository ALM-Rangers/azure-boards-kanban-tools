{
  "name": "kanban-board-tools",
  "private": true,
  "version": "0.0.0",
  "main": "webpack.config.js",
  "scripts": {
    "clean:vsix": "rimraf *.vsix",
    "clean": "npm run clean:vsix && rimraf dist",
    "dev": "npm run build:dev && webpack-dev-server -d --hot --progress --colors --https --content-base ./dist --port 9090",
    "lint": "tslint -c tslint.json \"src/*.ts\"",
    "compile:dev": "npm run clean:vsix && webpack --progress --config webpack.dev.config.js",
    "package:dev": "npm run clean:vsix && node ./scripts/packageDev",
    "publish:dev": "npm run package:dev && node ./scripts/packageDev",
    "build:dev": "npm run lint && npm run clean && mkdir dist && webpack --mode development --progress --config webpack.config.js --output-path ./dist",
    "build:release": "npm run lint && npm run clean && mkdir dist && webpack --progress --config webpack.prod.config.js --output-path ./dist",
    "compile:test": "npm run clean && webpack --progress --config webpack.config.js",
    "package:test": "npm run compile:test && node ./scripts/packageTest",
    "package:release": "npm run clean:vsix && npm run build:release && node ./scripts/packageRelease",
    "test": "snyk test && karma start --single-run",
    "start:dev": "webpack-dev-server --config webpack.dev.config.js"
  },
  "dependencies": {
    "applicationinsights-js": "^1.0.20",
    "azure-devops-extension-api": "~1.157.0",
    "azure-devops-extension-sdk": "~2.0.11",
    "azure-devops-ui": "~2.164.0",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "telemetryclient-team-services-extension": "^1.0.10"
  },
  "devDependencies": {
    "@types/jquery": "^3.2.18",
    "@types/react": "~16.8.2",
    "@types/react-dom": "~16.8.0",
    "copy-webpack-plugin": "^7.0.0",
    "base64-inline-loader": "~1.1.1",
    "css-loader": "~1.0.0",
    "file-loader": "^1.1.6",
    "node-sass": "^7.0.0",
    "rimraf": "~3.0.2",
    "sass-loader": "~10.0.5",
    "style-loader": "~0.23.1",
    "tfx-cli": "^0.9.2",
    "ts-loader": "~5.2.2",
    "typescript": "^3.9.6",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.0.0",
    "tslint": "^5.18.0",
    "tslint-loader": "^3.5.4",
    "url-loader": "^0.6.2"
  }
}
