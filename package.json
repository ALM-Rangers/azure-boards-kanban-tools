{
  "name": "kanban-board-tools",
  "private": true,
  "version": "0.0.0",
  "main": "webpack.config.js",
  "scripts": {
    "clean:vsix": "rimraf *.vsix",
    "clean": "npm run clean:vsix && rimraf dist",
    "dev": "webpack-dev-server --hot --progress --colors --https --port 9090",
    "lint": "tslint -c tslint.json \"src/*.ts",
    "package:dev": "npm run clean:vsix && node ./scripts/packageDev",
    "publish:dev": "npm run package:dev && node ./scripts/packageDev",
    "build:release": "npm run lint && npm run clean && mkdir dist && webpack --progress --colors --config webpack.prod.config.js --output-path ./dist -p",
    "package:release": "npm run clean:vsix && npm run build:release && node ./scripts/packageRelease",
    "test": "snyk test && karma start --single-run"
  },
  "devDependencies": {
    "@types/react": "~16.0.31",
    "@types/react-dom": "~16.0.3",
    "@types/jquery": "~3.2.17",
    "copy-webpack-plugin": "^4.3.0",
    "cpx": "^1.5.0",
    "file-loader": "^1.1.6",
    "node-sass": "^4.7.2",
    "rimraf": "^2.6.2",
    "react": "~16.2.0",
    "react-dom": "~16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "tfx-cli": "^0.4.16",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "~2.6.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "@uifabric/icons": "^5.3.0",
    "applicationinsights-js": "^1.0.14",
    "es6-promise": "^4.1.1",
    "office-ui-fabric-react": "~5.34.1",
    "telemetryclient": "0.0.5",
    "vss-ui": "~3.127.3",
    "vss-web-extension-sdk": "~4.126.2"
  }
}