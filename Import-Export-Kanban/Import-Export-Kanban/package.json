{
  "name": "import-export-kanban",
  "private": true,
  "version": "0.0.0",
  "main": "webpack.config.js",
  "scripts": {
    "clean:vsix": "rimraf *.vsix",
    "clean": "npm run clean:vsix && rimraf dist",
    "dev": "webpack-dev-server --hot --progress --colors --https --port 9090",
    "lint": "tslint -c tslint.json \"src/*.ts",
    "package:dev": "npm run clean:vsix && node ./scripts/packageDev",
    "publish:dev": "npm run package:dev && node ./scripts/packageDev",
    "build:release": "npm run lint && npm run clean && mkdir dist && webpack --progress --colors --config webpack.prod.config.js --output-path ./dist -p",
    "package:release": "npm run clean:vsix && npm run build:release && node ./scripts/packageRelease",
    "copy:libs": "cpx node_modules/vss-web-extension-sdk/lib/VSS.SDK.min.js libs && cpx node_modules/office-ui-fabric-js/dist/js/fabric.min.js libs && cpx node_modules/applicationinsights-js/dist/ai.0.js libs",
    "copy:css": "cpx node_modules/office-ui-fabric-js/dist/css/fabric.components.min.css css && cpx node_modules/office-ui-fabric-js/dist/css/fabric.min.css css",
    "test": "snyk test && karma start --single-run",
    "postinstall": "npm run copy:libs && npm run copy:css"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.0.1",
    "cpx": "^1.5.0",
    "jasmine": "^2.6.0",
    "node-sass": "^4.5.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "tfx-cli": "^0.4.5",
    "ts-loader": "^2.0.3",
    "tslint": "^5.2.0",
    "tslint-loader": "^3.5.3",
    "typescript": "~2.3.4",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "snyk": "^1.30.0"
  },
  "dependencies": {
    "applicationinsights-js": "^1.0.9",
    "es6-promise": "^4.1.0",
    "office-ui-fabric-js": "^1.4.0",
    "q": "^1.5.0",
    "telemetryclient": "0.0.5",
    "vss-web-extension-sdk": "^2.116.1"
  }
}