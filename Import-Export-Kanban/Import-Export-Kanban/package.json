{
  "name": "import-export-kanban",
  "private": true,
  "version": "0.0.0",
  "main": "webpack.config.js",
  "scripts": {
    "clean:vsix": "rimraf *.vsix",
    "clean": "npm run clean:vsix && rimraf dist",
    "dev": "webpack-dev-server --hot --progress --colors --https --port 9090",
    "lint": "tslint -c tslint.json \"src/*.ts",
    "package:dev": "npm run clean:vsix && node ./scripts/packageDev",
    "publish:dev": "npm run package:dev && node ./scripts/packageDev",
    "build:release": "npm run lint && npm run clean && mkdir dist && webpack --progress --colors --config webpack.prod.config.js --output-path ./dist -p",
    "package:release": "npm run clean:vsix && npm run build:release && node ./scripts/packageRelease",
    "copy:libs": "cpx node_modules/vss-web-extension-sdk/lib/VSS.SDK.min.js libs && cpx node_modules/q/q.js libs && cpx node_modules/applicationinsights-js/dist/ai.0.js libs",
    "test": "snyk test && karma start --single-run",
    "postinstall": "npm run copy:libs"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.0.1",
    "cpx": "^1.5.0",
    "jasmine": "^2.5.3",
    "node-sass": "^4.3.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "tfx-cli": "^0.3.45",
    "ts-loader": "^2.0.0",
    "tslint": "^4.3.1",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.5",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "snyk": "^1.24.6"
  },
  "dependencies": {
    "applicationinsights-js": "^1.0.7",
    "q": "^1.4.1",
    "telemetryclient": "0.0.4",
    "vss-web-extension-sdk": "^2.109.1"
  }
}
